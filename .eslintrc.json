{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser" : "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
    },
    "overrides": [
        {
            "files": ["*-test.js","*.spec.js"],
            "rules": {
                "no-unused-expressions": "off",
                "no-undef": "off",
                "no-unused-vars": "off",
                "import/no-unresolved": "off"
            }
        }
    ]
}
